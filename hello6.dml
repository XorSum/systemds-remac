
func  = function(matrix[double] a,matrix[double] b) return (matrix[double] ret) {
    tmp = a%*%b
    ret = tmp%*%t(tmp)+tmp
}

h = diag(matrix(1.0,rows=10,cols=1))
#h = y%*%t(y)
a = matrix(1.0,rows=10,cols=10)
i = 1
while (i<10) {
    i = i + 1
    h = h - a%*%h%*%t(h)%*%t(a)
    b = func(a,h)
    h = b%*%h%*%t(b)
}

print(toString(h))



